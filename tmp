        SHIFT PLANNING ce 28/12/2022 => reprendre ce 2023-05-26 fait ce 2023-07-21
            *********************************************************************************************************************************************************************
            CREATE TABLE shift (
              id_shift INT (15) PRIMARY KEY AUTO_INCREMENT,
              id_campaign INT NOT NULL,
              id_employe INT NOT NULL,
              start_time DATETIME NOT NULL,
              end_time DATETIME NOT NULL,
              FOREIGN KEY (id_campaign) REFERENCES campaign(id_campaign),
              FOREIGN KEY (id_employe) REFERENCES employe(idEmploye)
            );
            CREATE TABLE campaign (
              id_campaign INT PRIMARY KEY AUTO_INCREMENT,
              libelle VARCHAR (250) NOT NULL,
              start_date DATE NOT NULL,
              end_date DATE NOT NULL,
              start_time TIME NOT NULL,
              end_time TIME NOT NULL
            );

